{
  "name": "schul-cloud-content-client",
  "version": "0.3.1",
  "description": "Schul-Cloud Content",
  "author": "adrianjost <homepage@hackedit.de>",
  "private": true,
  "bundlesize": [
    {
      "path": "./dist/service-worker.js",
      "maxSize": "10 KB"
    },
    {
      "path": "./dist/static/**/[0-9]*.js",
      "maxSize": "30 KB"
    },
    {
      "path": "./dist/static/**/!([0-9])*.js",
      "maxSize": "500 KB"
    },
    {
      "path": "./dist/static/**/*.css",
      "maxSize": "50 KB"
    }
  ],
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "export": "rimraf component-export && cross-env NODE_ENV=production BABEL_ENV=export webpack --config ./build/webpack-export.config.js",
    "lint": "eslint --fix --ext .js,.vue src",
    "test": "bundlesize",
    "travis": "npm run build --report && npm run export && bundlesize"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "query-string": "^6.2.0",
    "vee-validate": "^2.0.0-rc.21",
    "vue": "^2.5.9",
    "vue-config": "^1.0.0",
    "vue-cookies": "^1.5.4",
    "vue-material": "latest",
    "vue-progressbar": "^0.7.3",
    "vue-resource": "^1.3.5",
    "vue-router": "^3.0.1",
    "vuejs-localization": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bundlesize": "^0.17.0",
    "chalk": "^2.1.0",
    "connect-history-api-fallback": "^1.4.0",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^8.0.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "node-sass": "^4.6.1",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "semver": "^5.4.1",
    "shelljs": "^0.8.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "uglify-es": "^3.3.9",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "url-loader": "^1.1.2",
    "vue-custom-element": "^3.2.6",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "*",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
