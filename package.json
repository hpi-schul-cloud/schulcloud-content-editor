{
  "name": "schul-cloud-content-client",
  "version": "0.3.1",
  "description": "Schul-Cloud Content",
  "author": "adrianjost <homepage@hackedit.de>",
  "private": true,
  "bundlesize": [
    {
      "path": "./dist/service-worker.js",
      "maxSize": "10 KB"
    },
    {
      "path": "./dist/static/**/[0-9]*.js",
      "maxSize": "30 KB"
    },
    {
      "path": "./dist/static/**/!([0-9])*.js",
      "maxSize": "500 KB"
    },
    {
      "path": "./dist/static/**/*.css",
      "maxSize": "50 KB"
    }
  ],
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "export": "rimraf component-export && cross-env NODE_ENV=production BABEL_ENV=export webpack --config ./build/webpack-export.config.js",
    "lint": "eslint --fix --ext .js,.vue src",
    "test": "bundlesize",
    "travis": "npm run build --report && npm run export && bundlesize"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "query-string": "^5.0.1",
    "vee-validate": "^2.0.0-rc.21",
    "vue": "^2.5.9",
    "vue-config": "^1.0.0",
    "vue-cookies": "^1.5.4",
    "vue-material": "latest",
    "vue-progressbar": "^0.7.3",
    "vue-resource": "^1.3.5",
    "vue-router": "^3.0.1",
    "vuejs-localization": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bundlesize": "^0.15.3",
    "chalk": "^2.1.0",
    "connect-history-api-fallback": "^1.4.0",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "node-sass": "^4.6.1",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "semver": "^5.4.1",
    "shelljs": "^0.8.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "uglify-es": "^3.3.9",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "url-loader": "^0.6.2",
    "vue-custom-element": "^1.0.13",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "*",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
